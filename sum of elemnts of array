#include <iostream>
using namespace std;
int sum_of_array(int a[],int n)
 {
    int sum=0;
    for(int i=0;i<n;i++)
    {
        sum=sum+a[i];
    }
    return sum;
 }

int main() {
 int a[100];
 int n,result;
 cout<<"enter the value of n"<<endl;
 cin>>n;
 cout<<"enter the elements of te array"<<endl;
 for (int i=0;i<n;i++)
 {
     cin>>a[i];
 }
 result= sum_of_array(a,n);
  cout<<"sum of elements of array is "<<endl;
  cout<< result;
  return 0;
}
